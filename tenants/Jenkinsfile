pipeline {
    agent any

    stages {
        stage ("Merge pull request") {
            steps {
                withCredentials([usernamePassword(
                    credentialsId: 'credential-value',
                    usernameVariable: 'ACCESS_TOKEN_USERNAME',
                    passwordVariable: 'ACCESS_TOKEN_PASSWORD',)]) {
                sh "curl -X PUT -d '{\"commit_title\": \"Merge pull request\"}'  https://github.com/api/v3/repos/pgombola/k8s-jenkins/pulls/$CHANGE_ID/merge?access_token=$ACCESS_TOKEN_PASSWORD"
        }
    }
}